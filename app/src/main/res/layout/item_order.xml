<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:layout_editor_absoluteX="-74dp"
    tools:layout_editor_absoluteY="-23dp">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginTop="20dp"
        android:text="Mã đơn: "
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toStartOf="@+id/tvOrderId"
        app:layout_constraintHorizontal_bias="0.911"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="123dp"
        android:layout_height="27dp"
        android:layout_marginTop="48dp"
        android:padding="5dp"
        android:text="Họ tên: "
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/tvFullName"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/tvOrderId"
        android:layout_width="274dp"
        android:layout_height="25dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="16dp"
        android:text="orderId"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvFullName"
        android:layout_width="269dp"
        android:layout_height="24dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="48dp"
        android:text="Fullname"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvPhoneOrder"
        android:layout_width="110dp"
        android:layout_height="24dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="36dp"
        android:text="Số điện thoại"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvOrderId"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="57dp"
        android:text="đ"
        android:textColor="#CA1414"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tvTotalPrice"
        app:layout_constraintTop_toBottomOf="@+id/tvAddressOrder"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="3dp"
        android:text="Địa chỉ: "
        android:textAlignment="viewEnd"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/tvAddressOrder"
        android:layout_width="284dp"
        android:layout_height="25dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="2dp"
        android:text="Địa chỉ"
        android:textColor="#724E4E"

        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPhoneOrder"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/tvDateReceived"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="32dp"
        android:text="dateReceive"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvAddressOrder"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_marginTop="76dp"
        android:text="SĐT: "
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="72dp"
        android:layout_height="22dp"
        android:layout_marginTop="184dp"
        android:text="Tổng tiền: "
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toEndOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/tvTotalPrice"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/tvTotalPrice"
        android:layout_width="76dp"
        android:layout_height="22dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="4dp"
        android:text="totalPrice"
        android:textColor="#CA1414"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDateReceived"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="112dp"
        android:layout_height="23dp"
        android:layout_marginTop="160dp"
        android:text="Dự kiến nhận:"
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/tvAddressOrder"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="113dp"
        android:layout_height="24dp"
        android:layout_marginTop="136dp"
        android:text="Ngày đặt:"
        android:textAlignment="viewEnd"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/tvAddressOrder"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

    </TextView>

    <TextView
        android:id="@+id/tvDateOrder"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="128dp"
        android:layout_marginTop="8dp"
        android:text="dateOrder"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvAddressOrder"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>